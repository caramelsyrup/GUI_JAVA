package com.model;

public class LoginTest {
	
	public static void main(String[] args) {
	
	}
	
	String url;
	String user; 
	String pwd;
	
	// 생성자- db 연결
	public LoginTest() {
		try {
			Class.forName("oracle.jdbc.driver.OracleDriver");
			url	= "jdbc:oracle:thin:@localhost:1521:xe";
			user = "scott";
			pwd = "1234";
		} catch (ClassNotFoundException e) {
			e.printStackTrace();
		}
	}

}
